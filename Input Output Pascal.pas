program InputOutputExample;
var
   inputString: string;
   inputNumber: Integer;

begin
   Write('Masukkan sebuah string: ');
   Readln(inputString);
   Writeln('String yang dimasukkan adalah: ', inputString);

   Write('Masukkan sebuah angka: ');
   Readln(inputNumber);
   Writeln('Angka yang dimasukkan adalah: ', inputNumber);

   Readln;
end.
